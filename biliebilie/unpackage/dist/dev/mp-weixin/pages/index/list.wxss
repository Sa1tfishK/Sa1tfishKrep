.list {
  height: 750px;
  background-color: #f5f5f5;
}
.list .listnav {
  background-color: white;
  height: 70px;
  width: 100%;
  padding-top: 20px;
  position: fixed;
  top: 0;
  z-index: 1;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.list .listnav  .uni-icons {
  margin: 0 10px;
}
.list .listscr {
  height: 650px;
  padding-top: 100px;
  background-color: #f5f5f5;
}
.list .listscr button::after {
  border: none;
}
.list .listscr text {
  font-size: 13px;
}
.list .listscr button {
  background-color: white;
  text-align: left;
  border-bottom: 1px #f3f3f3 solid;
}

