.presslist {
  height: 750px;
  background-color: #f5f5f5;
}
.presslist .plnav {
  background-color: #303030;
  height: 70px;
  width: 100%;
  padding-top: 20px;
  position: fixed;
  top: 0;
  z-index: 1;
  display: flex;
  align-items: center;
  color: white;
}
.presslist .plnav  .uni-icons {
  margin-left: 10px;
  margin-right: 20px;
}
.presslist .up {
  margin-top: 100px;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.presslist .up .uni-data-checklist {
  display: flex;
  justify-content: flex-end;
}
.presslist .up  .checklist-box.uni-label-pointer {
  margin-right: 0;
}
.presslist .up .uni-navbar__header-btns-right {
  padding-top: 20px;
}
.presslist .up  .checklist-text {
  font-size: 17px;
  line-height: 20px;
}
.presslist .down {
  height: 650px;
}
.presslist .down .plscr {
  height: 600px;
}
.presslist .down .plscr .press {
  display: flex;
  flex-direction: column;
  margin: 10px 5px;
  height: -webkit-fit-content;
  height: fit-content;
  background-color: white;
  border-bottom: 1px #f5f5f5 solid;
}
.presslist .down .plscr .press view {
  display: flex;
  height: 100px;
  margin: 10px 0px;
}
.presslist .down .plscr .press view view {
  margin: 0 10px;
  width: 70%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.presslist .down .plscr .press image {
  height: 50px;
  width: 50px;
  border-radius: 50%;
}
.presslist .nopress {
  height: 650px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.presslist .pressfooter {
  background-color: #f5f5f5;
  display: flex;
  align-items: center;
  position: fixed;
  bottom: 0;
  width: 98%;
  padding: 5px;
}

