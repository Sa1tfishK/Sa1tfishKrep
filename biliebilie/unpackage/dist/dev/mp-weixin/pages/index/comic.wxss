.comic {
  height: 750px;
  background-color: #f5f5f5;
}
.comic .comicnav {
  background-color: #353535;
  width: 100%;
  padding: 10px;
  padding-top: 40px;
  display: flex;
  align-items: center;
  position: fixed;
  top: 0;
  z-index: 1;
}
.comic .comicnav label {
  color: white;
  font-size: 15px;
  margin-left: 10px;
}
.comic .comicmenu {
  background-color: #353535;
  width: 95%;
  padding: 10px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: fixed;
  bottom: 0;
}
.comic .comicmenu label {
  color: white;
  font-size: 15px;
}
.comic .comicmenu view {
  display: flex;
  align-items: center;
}

