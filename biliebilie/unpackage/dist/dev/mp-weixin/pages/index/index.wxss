.content {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
}
.content .box-bg .mynav .uni-px-5 {
  width: 100%;
  display: flex;
  padding-top: 20px;
  align-items: center;
  justify-content: center;
}
.content .box-bg .mynav .uni-px-5 .uni-data-checklist {
  display: flex;
  justify-content: center;
}
.content .box-bg .mynav .uni-px-5  .checklist-box {
  margin: 0;
}
.content .box-bg .mynav .uni-px-5 .uni-navbar__header-btns-right {
  padding-top: 20px;
}
.content .box-bg .mynav .uni-px-5  .checklist-text {
  font-size: 17px;
  line-height: 20px;
}
.content .indexsc {
  height: 610px;
}
.content .indexsc .bookshelf {
  height: 150px;
  margin: 10px;
  display: flex;
  align-items: center;
  border-bottom: 1px #f5f5f5 solid;
}
.content .indexsc .bookshelf image {
  height: 140px;
  width: 100px;
  margin-right: 10px;
}
.content .indexsc .bookshelf  .uni-icons {
  position: absolute;
  right: 5px;
}
.content .indexsc .bookshelf view {
  display: flex;
  height: 70%;
  flex-direction: column;
  justify-content: space-around;
  align-items: flex-start;
}
.content .inpop {
  background-color: white;
  display: flex;
  flex-direction: column;
}
.content .inpop view:nth-child(1) {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 10px;
}
.content .inpop view:nth-child(1) view:nth-child(1) {
  display: flex;
  justify-content: flex-start;
  width: 80%;
}
.content .inpop view:nth-child(1) view:nth-child(1) view {
  display: flex;
  flex-direction: column;
  margin-left: 10px;
}
.content .inpop view:nth-child(2) {
  display: flex;
  justify-content: space-around;
}
.content .inpop view:nth-child(2) view {
  display: flex;
  flex-direction: column;
  font-size: 10px;
  justify-content: flex-start;
}

